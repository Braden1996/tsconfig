{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native",
  "extends": "./react-native-library.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
